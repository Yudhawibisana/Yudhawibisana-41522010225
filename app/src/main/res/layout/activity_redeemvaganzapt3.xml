<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".redeemvaganzapt3"
    android:background="@drawable/group_14">

    <RelativeLayout
        android:id="@+id/group_385"
        android:layout_width="220dp"
        android:layout_height="31dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="11dp"
        android:layout_marginTop="12dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <Button
            android:id="@+id/vector"
            android:layout_width="25dp"
            android:layout_height="20dp"
            android:layout_alignParentStart="true"
            android:layout_marginStart="0dp"
            android:layout_marginTop="8dp"
            android:background="@drawable/button2"
            android:onClick="main_menu"
            tools:ignore="SpeakableTextPresentCheck,TouchTargetSizeCheck" />

        <TextView
            android:id="@+id/scan_tutup_"
            android:layout_width="198dp"
            android:layout_height="match_parent"
            android:layout_alignParentEnd="true"
            android:textColor="#4D4D4D"
            android:layout_marginEnd="0dp"
            android:gravity="center_horizontal|center_vertical"
            android:lineSpacingExtra="3sp"
            android:text="Redeem Vaganza"
            android:fontFamily="@font/montserratbold"
            android:textSize="15dp"/>

    </RelativeLayout>

    <ImageView
        android:layout_width="54dp"
        android:layout_height="18dp"
        android:layout_marginTop="570dp"
        android:layout_marginEnd="24dp"
        android:background="@drawable/pls_mns"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/some_id"
        android:layout_width="31dp"
        android:layout_height="30dp"
        android:layout_marginTop="580dp"
        android:layout_marginEnd="36dp"
        android:gravity="center_vertical"
        android:lineSpacingExtra="12sp"
        android:fontFamily="@font/montserratsemibold"
        android:text="400"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/rectangle_11"
        android:layout_width="347dp"
        android:layout_height="70dp"
        android:layout_marginTop="630dp"
        android:background="@drawable/btn01"
        android:onClick="vaganzadetail"
        android:text="Redeem"
        android:textAllCaps="false"
        android:textColor="@color/white"
        android:textSize="18dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />


</androidx.constraintlayout.widget.ConstraintLayout>